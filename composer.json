{
    "name": "jdavidbakr/laravel-cache-garbage-collector",
    "type": "library",
    "description": "A script that will clean up expired cache files if the system is using the files cache system",
    "keywords": [
        "jdavidbakr",
        "LaravelCacheGarbageCollector"
    ],
    "homepage": "https://github.com/jdavidbakr/LaravelCacheGarbageCollector",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Baker",
            "email": "me@jdavidbaker.com",
            "homepage": "http://www.jdavidbaker.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.5|6.*|7.*",
        "php" : ">=5.4.0",
        "mockery/mockery": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*|5.*|6.*|7.*|8.*",
        "squizlabs/php_codesniffer": "~2.3",
        "orchestra/testbench": "3.*|4.*|5.*"
    },
    "autoload": {
        "psr-4": {
            "jdavidbakr\\LaravelCacheGarbageCollector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jdavidbakr\\LaravelCacheGarbageCollector\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
